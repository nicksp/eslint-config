{
  "name": "@nicksp/eslint-config",
  "description": "Nick's ESLint config",
  "version": "1.5.7",
  "type": "module",
  "packageManager": "pnpm@10.29.3",
  "author": {
    "name": "Nick Plekhanov",
    "url": "https://plekhanov.me/"
  },
  "license": "MIT",
  "funding": "https://buymeacoffee.com/plekhanov",
  "homepage": "https://github.com/nicksp/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicksp/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/nicksp/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "styleguide",
    "eslint-config"
  ],
  "main": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "nr gen && tsdown --clean --dts",
    "dev": "npx @eslint/config-inspector --config eslint-inspector.config.ts",
    "watch": "tsdown --watch",
    "lint": "npx publint && eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"**/*.{js,cjs,mjs,jsx,ts,tsx,astro,css,json}\"",
    "gen": "tsx scripts/typegen.ts",
    "prepack": "NODE_ENV=production nr build",
    "test": "nr test:ci",
    "test:ci": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "check": "nr typecheck && nr lint",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^2.13.0",
    "@next/eslint-plugin-next": "^16.1.6",
    "@nicksp/prettier-config": "^1.1.1",
    "astro-eslint-parser": "^1.3.0",
    "eslint": "^10.0.0",
    "eslint-plugin-astro": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "prettier-plugin-astro": "^0.14.1"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@nicksp/prettier-config": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^1.0.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/utils": "^8.55.0",
    "@vitest/eslint-plugin": "^1.6.9",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-flat-config-utils": "^3.0.1",
    "eslint-plugin-antfu": "^3.2.2",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-erasable-syntax-only": "^0.4.0",
    "eslint-plugin-import-lite": "^0.5.1",
    "eslint-plugin-jsdoc": "^62.5.5",
    "eslint-plugin-jsonc": "^3.0.0",
    "eslint-plugin-n": "^17.24.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-regexp": "^3.0.0",
    "eslint-plugin-unicorn": "^63.0.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "eslint-plugin-yml": "^1.19.1",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^2.4.2",
    "local-pkg": "^1.1.2",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.55.0",
    "yaml-eslint-parser": "^1.3.2"
  },
  "devDependencies": {
    "@antfu/ni": "^28.2.0",
    "@eslint-react/eslint-plugin": "^2.13.0",
    "@eslint/config-inspector": "^1.4.2",
    "@next/eslint-plugin-next": "^16.1.6",
    "@nicksp/eslint-config": "workspace:*",
    "@nicksp/prettier-config": "^1.1.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.2.3",
    "astro-eslint-parser": "^1.3.0",
    "eslint": "^10.0.0",
    "eslint-plugin-astro": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-typegen": "^2.3.0",
    "execa": "^9.6.1",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier-plugin-astro": "^0.14.1",
    "simple-git-hooks": "^2.13.1",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "prettier": "@nicksp/prettier-config"
}
